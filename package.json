{
  "name": "@mojis/loomicode",
  "version": "0.2.0-beta.7",
  "description": "A loom for unicode data files",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.11.1",
  "license": "MIT",
  "homepage": "https://github.com/mojisdev/loomicode",
  "repository": "https://github.com/mojisdev/loomicode",
  "bugs": {
    "url": "https://github.com/mojisdev/loomicode/issues"
  },
  "keywords": [
    "loom",
    "unicode",
    "data files"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./helpers": "./dist/helpers.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate:data-files": "tsx scripts/generate-data-files.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "arktype": "^2.1.17",
    "compare-versions": "^6.1.1"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.18.1",
    "@types/node": "^22.10.10",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.23.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
